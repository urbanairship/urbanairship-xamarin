<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
   <metadata>
      <id>urbanairship.portable</id>
      <version>4.2.1</version>
      <title>Urban Airship SDK PCL</title>
      <authors>Urban Airship, Inc.</authors>
      <requireLicenseAcceptance>false</requireLicenseAcceptance>
      <description>A portable client library for the Urban Airship SDK.</description>

      <dependencies>
          <group targetFramework="MonoAndroid">
            <dependency id="urbanairship" version="4.2.1"/>
	  </group>

	  <group targetFramework="Xamarin.iOS">
	    <dependency id="urbanairship" version="4.1.1"/>
	  </group>
      </dependencies>
   </metadata>

   <files>
        <!-- Core files -->
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable/bin/release/AirshipBindings.Portable.dll"
            target="lib/portable-net45+wp8+wpa81+win8+MonoAndroid41+MonoTouch10+Xamarin.iOS10/AirshipBindings.Portable.dll" />
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.Abstractions/bin/release/AirshipBindings.Portable.Abstractions.dll"
            target="lib/portable-net45+wp8+wpa81+win8+MonoAndroid41+MonoTouch10+Xamarin.iOS10/AirshipBindings.Portable.Abstractions.dll" />

        <!-- Android -->
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.Android/bin/release/AirshipBindings.Portable.dll"
            target="lib/MonoAndroid41/AirshipBindings.Portable.dll" />
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.Abstractions/bin/release/AirshipBindings.Portable.Abstractions.dll"
            target="lib/MonoAndroid41/AirshipBindings.Portable.Abstractions.dll" />

        <!-- iOS -->
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.iOS/bin/release/AirshipBindings.Portable.dll"
            target="lib/Xamarin.iOS10/AirshipBindings.Portable.dll" />
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.Abstractions/bin/release/AirshipBindings.Portable.Abstractions.dll"
            target="lib/Xamarin.iOS10/AirshipBindings.Portable.Abstractions.dll" />
   </files>
</package>
