<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
   <metadata>
      <id>urbanairship.portable</id>
      <version>12.0.0</version>
      <title>Airship SDK Portable Client Library</title>
      <authors>Urban Airship, Inc.</authors>
      <projectUrl>https://github.com/urbanairship/urbanairship-xamarin</projectUrl>
      <requireLicenseAcceptance>false</requireLicenseAcceptance>
      <description>A portable client library for the Airship SDK.</description>

      <dependencies>
         <group targetFramework="MonoAndroid">
            <dependency id="urbanairship.android.core" version="10.1.1"/>
         </group>

         <group targetFramework="Xamarin.iOS">
            <dependency id="urbanairship.ios.core" version="13.0.4"/>
            <dependency id="urbanairship.ios.automation" version="13.0.4"/>
            <dependency id="urbanairship.ios.extendedactions" version="13.0.4"/>
            <dependency id="urbanairship.ios.messagecenter" version="13.0.4"/>
         </group>
      </dependencies>
   </metadata>

   <files>
        <!-- Core files -->
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable/bin/release/AirshipBindings.Portable.dll"
            target="lib/portable-net45+wp8+wpa81+win8+MonoAndroid41+MonoTouch10+Xamarin.iOS10" />
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.Abstractions/bin/release/AirshipBindings.Portable.Abstractions.dll"
            target="lib/portable-net45+wp8+wpa81+win8+MonoAndroid41+MonoTouch10+Xamarin.iOS10" />

        <!-- Android -->
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.Android/bin/release/AirshipBindings.Portable.dll"
            target="lib/MonoAndroid41" />
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.Abstractions/bin/release/AirshipBindings.Portable.Abstractions.dll"
            target="lib/MonoAndroid41" />

        <!-- iOS -->
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.iOS/bin/release/AirshipBindings.Portable.dll"
            target="lib/Xamarin.iOS10" />
        <file src="src/AirshipBindings.Portable/AirshipBindings.Portable.Abstractions/bin/release/AirshipBindings.Portable.Abstractions.dll"
            target="lib/Xamarin.iOS10" />
   </files>
</package>
