<?xml version="1.0" encoding="utf-8"?>

<Project>

  <PropertyGroup>
    <_AirshipPreferenceCenterId>AirshipPreferenceCenter</_AirshipPreferenceCenterId>
  </PropertyGroup>

  <ItemGroup Condition="('$(OutputType)' != 'Library' OR '$(IsAppExtension)' == 'True') AND ('$(TargetFrameworks)' == '' AND '$(TargetFramework)' == '')">
    <_NativeReference Include="$(MSBuildThisFileDirectory)..\lib\xamarinios10\airship.ios.preferencecenter.resources\AirshipPreferenceCenter.xcframework">
        <_Id>$(_AirshipPreferenceCenterId)</_Id>
    </_NativeReference>
  </ItemGroup>
  <Target Name="_ResolveNativeReferencesForAirshipPreferenceCenterBinding" 
          BeforeTargets="ResolveNativeReferences"
          Condition="('$(TargetFrameworks)' == '' AND '$(TargetFramework)' == '') OR '$(IsBindingProject)' == 'true'">
    
    <ItemGroup>
      <_NativeReference Update="@(_NativeReference)" Condition="'%(_NativeReference._Id)' == '$(_AirshipPreferenceCenterId)'">
        <Kind>Framework</Kind>
        <SmartLink>False</SmartLink>
        <!-- <ForceLoad>True</ForceLoad> -->
        <!-- <LinkerFlags>-ObjC</LinkerFlags> -->
      </_NativeReference>
      
      <NativeReference Include="@(_NativeReference)" />
    </ItemGroup>
  </Target>

</Project>

