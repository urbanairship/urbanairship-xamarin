<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <Target Name="AirshipMessageCenterBeforeCompile" AfterTargets="BeforeCompile">
        <ItemGroup>
            <BindingResources Include="$(MSBuildThisFileDirectory)../content/AMC.resources/**/*.*" />
        </ItemGroup>
        <Copy SourceFiles="@(BindingResources)" DestinationFolder="$(TargetDir)/AirshipBindings.iOS.MessageCenter.resources/%(RecursiveDir)" ContinueOnError="false" />
        <ItemGroup>
            <NativeReference Include="$(TargetDir)/AirshipBindings.iOS.MessageCenter.resources\AirshipMessageCenter.xcframework">
                <Kind>Framework</Kind>
                <SmartLink>False</SmartLink>
            </NativeReference>
        </ItemGroup>
    </Target>
</Project>
