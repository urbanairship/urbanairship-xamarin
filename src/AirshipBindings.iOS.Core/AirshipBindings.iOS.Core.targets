<?xml version="1.0" encoding="utf-8"?>

<Project>
  <PropertyGroup>
    <_AirshipCoreId>AirshipCore</_AirshipCoreId>
  </PropertyGroup>

  <ItemGroup Condition="('$(OutputType)' != 'Library' OR '$(IsAppExtension)' == 'True') AND ('$(TargetFrameworks)' == '' AND '$(TargetFramework)' == '')">
    <_NativeReference Include="$(MSBuildThisFileDirectory)..\lib\xamarinios10\AirshipCore.iOS.resources\AirshipCore.xcframework">
        <_Id>$(_AirshipCoreId)</_Id>
    </_NativeReference>
  </ItemGroup>
  <Target Name="_ResolveNativeReferencesForAirshipCoreBinding" 
          BeforeTargets="ResolveNativeReferences"
          Condition="('$(TargetFrameworks)' == '' AND '$(TargetFramework)' == '') OR '$(IsBindingProject)' == 'true'">
    <ItemGroup>
      <_NativeReference Update="@(_NativeReference)" Condition="'%(_NativeReference._Id)' == '$(_AirshipCoreId)'">
        <Kind>Framework</Kind>
        <SmartLink>False</SmartLink>
        <!-- <ForceLoad>True</ForceLoad> -->
        <!-- <LinkerFlags></LinkerFlags> -->
      </_NativeReference>
      <NativeReference Include="@(_NativeReference)" />
    </ItemGroup>
  </Target>

</Project>

